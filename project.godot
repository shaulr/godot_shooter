; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

recent_projects=["res://quests/tutorial.quest", "res://addons/quest_manager/Examples/ExampleQuests.quest"]
quest_file_path="res://quests/tutorial.quest"

[application]

config/name="Shooter"
run/main_scene="res://UI/main_menu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Game="*res://game.gd"
scene_manager="*res://world/scene_manager.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"
QuestManager="*res://addons/quest_manager/QuestManager.gd"
CampaignManager="*res://quests/campaign_manager.gd"

[dialogic]

directories/dch_directory={
"Bata": "res://quests/dialogs/Bata.dch",
"Marko": "res://quests/dialogs/Marko.dch",
"Tito": "res://quests/dialogs/Tito.dch",
"bosko_buha": "res://quests/dialogs/bosko_buha.dch",
"stanojlo": "res://quests/dialogs/stanojlo.dch",
"unit_test_character": "res://Tests/Resources/unit_test_character.dch",
"zdenka": "res://quests/dialogs/zdenka.dch"
}
directories/dtl_directory={
"zdenka_marko": "res://quests/dialogs/zdenka_marko.dtl"
}
glossary/default_case_sensitive=true
layout/style_list=["res://quests/dialogs/dialog_style.tres"]
layout/default_style="res://quests/dialogs/dialog_style.tres"
variables={}
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}

[display]

window/size/viewport_width=1024
window/size/viewport_height=580
window/size/mode=3
window/size/borderless=true
window/size/window_width_override=1024
window/size/window_height_override=580
window/stretch/mode="canvas_items"
mouse_cursor/custom_image="res://art/aimCursor.png"

[dotnet]

project/assembly_name="Shooter"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg", "res://addons/quest_manager/plugin.cfg")

[gui]

theme/custom="res://UI/partisan_theme.tres"

